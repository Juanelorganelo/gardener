{
  "name": "@ck/gardener",
  "description": "A simple linter for Git branches",
  "version": "0.0.0",
  "author": "Juan Manuel @Juanelorganelo",
  "bin": {
    "gardener": "./bin/run"
  },
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc --extension .ts mocha \"src/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/course-key/blinter/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "ajv": "^8.4.0",
    "tslib": "^2.2.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/course-key/gardener",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "blinter"
  },
  "repository": "course-key/gardener"
}
